@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(Threads REQUIRED)
find_dependency(Eigen3 REQUIRED)

# Optional dependencies
if(@TURBOINFER_ENABLE_OPENMP@)
    find_dependency(OpenMP REQUIRED)
endif()

# Include targets
include("${CMAKE_CURRENT_LIST_DIR}/TurboInferTargets.cmake")

# Set variables for users
set(TurboInfer_FOUND TRUE)
set(TurboInfer_VERSION "@PROJECT_VERSION@")

check_required_components(TurboInfer)
